{
    
    "builds": [
      {
        "src": "ipd/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb","runtime":"python 3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        
        "dest": "ipd/wsgi.py"
      }
      
    ]
  }
  